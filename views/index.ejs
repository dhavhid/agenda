<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<link href='http://fonts.googleapis.com/css?family=Patrick+Hand+SC|Galdeano' rel='stylesheet' type='text/css'>
		<link rel='stylesheet' href='/stylesheets/bootstrap.min.css' />
		<link rel='stylesheet' href='/stylesheets/bootstrap-responsive.min.css' />
		<link rel='stylesheet' href='/stylesheets/magnificpopup.css' />
		<link rel='stylesheet' href='/stylesheets/style.css' />
		<script type="text/javascript" src="/javascripts/jquery.min.js"></script>
		<script type="text/javascript" src="/javascripts/bootstrap.min.js"></script>
		<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>
		<script type="text/javascript" src="/javascripts/map.js"></script>
		<script type="text/javascript" src="/javascripts/jquery.magnific-popup.min.js"></script>
		<script type="text/javascript" src="/javascripts/global.js"></script>
	</head>
	<body>
	<div class="container-fluid">
		<div class="row-fluid"> <!-- row fluid 1 -->
			<div class="navbar navbar-inverse navbar-fixed-top"> <!-- navbar -->
				<div class="navbar-inner">
					<div class="container">
						
						<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
							<span class="icon-bar"></span>
					        <span class="icon-bar"></span>
					        <span class="icon-bar"></span>
						</a>
						
						<a class="brand" href="#">Agenda</a>
						
						<div class="nav-collapse collapse">
					    	<ul class="nav">
								<li class="active">
									<a href="#">Contacts</a>
								</li>
								<li><a href="#">Search</a></li>
								<li><a href="#">Log out</a></li>
							</ul>
					    </div>
					    
					</div> <!-- end of container -->
				</div>
			</div> <!-- navbar -->
		</div> <!-- end of row fluid 1 -->
		<br /><br /><br />
		<div class="row-fluid">
			<div class="span3">
				<input type="button" value="Add Contact" class="btn btn-primary">
				&nbsp;&nbsp;
				<input type="button" value="Search Contact" class="btn">
			</div>
		</div>
		<br />
		<div class="row-fluid">
			<div class="span7">
				<table class="table table-striped table-hover">
					<thead>
						<tr>
							<th width="5%">*</th>
							<th width="55%" colspan="2" style="text-align:center !important;">NAME</th>
							<th width="20%">TELEPHONE</th>
							<th width="20%">E-MAIL</th>
							<th width="5%"></th>
						</tr>
					</thead>
					<tbody>
						<% for( var i = 0; i<contacts.length; i++ ){ %>
						<tr>
							<td><%= i+1 %></td>
							<td style="text-align:center;"><% if(contacts[i].profile_picture){ %>
									<a href="/contacts/picture/<%= contacts[i]._id.toHexString() %>" class="magnific">
									<img src="/contacts/picture/<%= contacts[i]._id.toHexString() %>" class="img-polaroid" width="50px" />
									</a>
								<%}else{%>
									<img class="img-polaroid" width="50px" src="data:image/png;base64," />
								<%}%>
							</td>
							<td><%= contacts[i].firstname %></td>
							<td><%= contacts[i].phone %></td>
							<td><%= contacts[i].email %></td>
							<td><a href="/contacts/edit/<%= contacts[i]._id.toHexString() %>" class="btn">Edit</a></td>
						</tr>
						<% } %>
					</tbody>
					<tfoot>
					</tfoot>
				</table>
			</div> <!-- end of span7 -->
			<div class="span5">
				<div id="map-canvas"></div>
			</div>
		</div>
	</div> <!-- end of container -->
	</body>
</html>